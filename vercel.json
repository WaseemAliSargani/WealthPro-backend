{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/users/(.*)", "dest": "api/index.js" },
    { "src": "/api/transactions/(.*)", "dest": "api/index.js" },
    { "src": "/api/(.*)", "dest": "api/index.js" },
    { "src": "/", "dest": "api/index.js" }
  ]
}